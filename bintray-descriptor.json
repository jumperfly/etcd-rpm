{
  "package": {
    "name": "etcd",
    "repo": "yum",
    "subject": "jumperfly",
    "desc": "etcd",
    "vcs_url": "https://github.com/jumperfly/etcd-rpm.git",
    "licenses": ["Apache-2.0"]
  },

  "version": {
    "name": "${RPM_VERSION}"
  },

  "files": [{
      "includePattern": "build/RPMS/x86_64/(etcd-.*\\.rpm)",
      "uploadPattern": "etcd/${YUM_PATH}/$1"
    }
  ],
  "publish": true
}
